    "bugs": {
      "url": "https://github.com/spencer-carver/mtgdb/issues"
    },
    "homepage": "https://github.com/spencer-carver/mtgdb#readme",
    "dependencies": {
      "aws-sdk": "^2.503.0"
    }
  }
  